{
  "{asid}": {
    "profileList": ["AsidAuthorities", "AsidOperatives", "AsidLicenseOwner", "Public"],
    "useCaseMap": {}
  },
  "*": {
    "profileList": [
      "Authorities",
      "Operatives",
      "AwidLicenseOwner",
      "Public",
      "Auditors",
      "Authenticated",
      "AwidInitiator",
      "Autherities"
    ],
    "useCaseMap": {
      "sys/uuAppWorkspace/init": {
        "sysStateList": ["created", "assigned"],
        "profileList": ["AwidLicenseOwner", "AwidInitiator"]
      },
      "sys/uuAppWorkspace/load": {
        "sysStateList": ["created", "assigned", "active", "restricted", "readOnly", "closed", "suspended"],
        "profileList": ["Authenticated"]
      },
      "sys/uuAppWorkspace/loadBasicData": {
        "sysStateList": ["created", "assigned", "active", "restricted", "readOnly", "closed", "suspended"],
        "profileList": ["Public"]
      },
      "defaultUve": {
        "profileList": ["Public"]
      },
      "sys/uuAppWorkspace/initUve": {
        "sysStateList": ["created", "assigned"],
        "profileList": ["Public", "AwidLicenseOwner", "AwidInitiator"]
      },
      "sys/uuAppWorkspace/apiConsole": {
        "sysStateList": ["created", "assigned", "active", "restricted", "readOnly", "closed", "suspended"],
        "profileList": ["Authenticated"]
      },
      "device/list": {
        "sysStateList": ["active"],
        "profileList": ["Authorities", "Operatives", "Auditors"]
      },
      "device/create": {
        "sysStateList": ["active"],
        "profileList": ["Authorities", "Operatives"]
      },
      "device/delete": {
        "sysStateList": ["active"],
        "profileList": ["Authorities", "Operatives"]
      }
    }
  }
}
